<%= simple_form_for(@guard) do |f| %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :specialty %>
    <%= f.input :rate %>
    <%= f.input :cover, label: 'Cover Photo' %>
    <%= f.input :cover_cache, as: :hidden %>
    <%= f.input :picture, label: 'Profile Picture' %>
    <%= f.input :picture_cache, as: :hidden %>
    <%= f.input :location, label: 'Head Quarter Location' %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Create", class:'btn-green btn-large' %>
  </div>
<% end %>
