function completeMission() {
  const completedListItem = document.getElementById('<%= @booking.id %>');
  console.log(completedListItem);
  <% if @booking.guard.user == current_user %>
    const html = "<%= j render 'components/list-item-guard-view', booking: @booking %>";
  <% else %>
    const html = "<%= j render 'components/list-item', booking: @booking %>";
  <% end %>
  completedListItem.innerHTML = html;
}

completeMission();
